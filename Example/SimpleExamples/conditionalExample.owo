"single line if";

nani(uwu) 
    baka("yes");


"single line if/else";

nani(uwu) 
    baka("yes");
noU 
    baka("no");


"block if/else";

nani(uwu){
    baka("yes");
} noU {
    baka("no");
}


"block if -> else if -> else";

nani(uwu){
    baka("yes");
} noU nani (uwu) {
    baka("maybe");
} noU {
    baka("no");
}


